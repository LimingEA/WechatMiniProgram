<image class="bg" src="/images/招聘会图片4.png">
</image>
<form bindsubmit="formSubmit" report-submit>
  <view class="content">
    <view>
      <input name="name" bindblur="nameChange" placeholder-class="phcolor" placeholder="输入您的姓名" />
    </view>
    <view>
      <input name="phone" bindblur="phoneChange" placeholder-class="phcolor" placeholder="输入您的手机号码" />
    </view>
   
    
    <button form-type="submit">提交</button>
  </view>
</form>
